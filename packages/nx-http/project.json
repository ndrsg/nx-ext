{
  "sourceRoot": "packages/nx-http/src",
  "projectType": "library",
  "targets": {
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "push": true,
        "noVerify": true,
        "postTargets": [
          "nx-http:publish",
          "nx-http:github"
        ]
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}",
        "title": "@ndrsg/nx-http@${version}"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-http",
        "main": "packages/nx-http/src/index.ts",
        "tsConfig": "packages/nx-http/tsconfig.lib.json",
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          "README.md",
          "packages/nx-http/CHANGELOG.md",
          {
            "input": "./packages/nx-http/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-http/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-http",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-http",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nx-http/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nx-http"],
      "options": {
        "jestConfig": "packages/nx-http/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
