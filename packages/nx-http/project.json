{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nx-http/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-http",
        "main": "packages/nx-http/src/index.ts",
        "tsConfig": "packages/nx-http/tsconfig.lib.json",
        "assets": [
          "packages/nx-http/*.md",
          {
            "input": "./packages/nx-http/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-http/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-http",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-http",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "download": {
      "executor": "./dist/packages/nx-http:download",
      "options": {
        "url": "https://webhook.site/token/5c0e348f-d188-4e54-8f6f-79efc75a87fe/request/0138ee58-d9a2-408e-b303-07557c759bfb/download/32702fb1-135f-4fd1-bcb0-a3fecda98a26",
        "targetPath": "./fileoutput/download.md"
      }
    },
    "hook": {
      "executor": "./dist/packages/nx-http:request",
      "options": {
        "url": "https://webhook.site/5c0e348f-d188-4e54-8f6f-79efc75a87fe",
        "method": "POST",
        "data": {
          "custom": "data"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "query": {
          "queryparam1": "1",
          "queryparam2": "2"
        },
        "responseFilePath": "./fileoutput/test-hook.json"
      }
    },
    "upload": {
      "executor": "./dist/packages/nx-http:upload",
      "options": {
        "url": "https://webhook.site/5c0e348f-d188-4e54-8f6f-79efc75a87fe/07cbd7de-0b89-412a-b29f-66ec78f1693b",
        "sourcePath": "./packages/nx-http/README.md",
        "headers": {}
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nx-http/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nx-http"],
      "options": {
        "jestConfig": "packages/nx-http/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
